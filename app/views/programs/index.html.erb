<h1>Programas Registrados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= link_to 'Ver Info', program %></td>
        <td><%= link_to 'Editar', edit_program_path(program) %></td>
        <td><%= link_to 'Eliminar', program, method: :delete, data: { confirm: 'Â¿Eliminar Programa?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

